DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DATABASE?sslmode=require

# Optional (client): if set, enables Google login via Google Identity Services
VITE_GOOGLE_CLIENT_ID=

# Optional (client): base URL for API calls when running locally (e.g. via `vercel dev`)
VITE_API_BASE_URL=http://localhost:3000

# Optional (client): Truecaller SDK (only if you have a web integration)
VITE_TRUECALLER_SDK_URL=
VITE_TRUECALLER_PARTNER_KEY=

# Optional (client): analytics endpoint (receives POST JSON). If unset, analytics is disabled.
VITE_ANALYTICS_ENDPOINT=